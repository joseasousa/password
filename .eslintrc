{
    "parser": "babel-eslint",
    "env": {
        "node": true,
        "browser": true,
        "es6": true,
        "jest": true
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "extends": ["airbnb"],
    "rules": {
        "react/jsx-filename-extension": [
            "error",
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "global-require": "off",
        "no-console": "off",
        "import/prefer-default-export": "off"
    }
}